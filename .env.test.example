# E2E Test Environment Configuration
# Copy this file to .env.test and fill in your test Supabase project values

# Supabase Test Project (Cloud)
SUPABASE_URL=https://your-test-project.supabase.co
PUBLIC_SUPABASE_URL=https://your-test-project.supabase.co
SUPABASE_KEY=your-service-role-key
PUBLIC_SUPABASE_KEY=your-anon-key

# Test User Credentials (created manually in Supabase Dashboard)
E2E_USERNAME_ID=test-user-uuid
E2E_USERNAME=test@example.com
E2E_PASSWORD=your-test-password

# OpenRouter Configuration for Tests
OPENROUTER_API_KEY=your-openrouter-key
OPENROUTER_USE_MOCK=true

# Base URL for E2E tests
BASE_URL=http://localhost:3000
